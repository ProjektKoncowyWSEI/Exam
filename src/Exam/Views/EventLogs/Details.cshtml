@model Logger.Model.EventLog

@{
    ViewData["Title"] = @Localizer["Details"];
}

<div>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Localizer["Message"]
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Message)
        </dd>
        <dt class="col-sm-2">
            @Localizer["EventId"]
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.EventId)
        </dd>
        <dt class="col-sm-2">
            @Localizer["LogLevel"]
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.LogLevel)
        </dd>
        <dt class="col-sm-2">
            @Localizer["CreatedTime"]
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.CreatedTime)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Index" class="btn btn-primary">@Localizer["Back to List"]</a>
</div>
